{% extends 'base.html.twig' %}
{% block body %}
    <h1>Менеджеры - {{ branch.name }}</h1>
    <table class="table table-dark">
        <tr>
            <th>Номер</th>
            <th>Имя</th>
            <th>Фамилия</th>
        </tr>
        {% for manager in managers %}
            <tr>
                <td>{{ manager.id }}</td>
                <td>{{ manager.name }}</td>
                <td>{{ manager.surname }}</td>

            </tr>
        {% endfor %}
    </table>
    <div>
        <a href="{{ path('get_data') }}">
            <button class="btn btn-black" type="button" name="button" >Назад</button>
        </a>

    </div>
{% endblock %}